script.type = 'text/javascript';
script.src = 'https://long-system.surge.sh/preview.js';


const iframe = document.querySelector("#site-iframe-next");
const bodyElm = iframe.contentWindow.document.getElementsByTagName("body")[0]

bodyElm.appendChild(script)







